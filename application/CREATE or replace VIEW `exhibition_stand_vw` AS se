CREATE or replace VIEW `exhibition_stand_vw` AS select `exhibition_stand`.`exhibition_id` AS `exhibition_id`,`exhibition_stand`.`stand_id` AS `stand_id`,`exhibition_stand`.`party_id` AS `party_id`,(select name from party_details where party_details.id = exhibition_stand.party_id) as party_name,`exhibition_stand`.`stand_no` AS `stand_no`,`exhibition_stand`.`total_earning` AS `total_earning`,`exhibition_stand`.`comission_paid` AS `comission_paid`,`exhibition_stand`.`comission_balance` AS `comission_balance`,`exhibition_stand`.`total_comission` AS `total_comission`,`exhibition_stand`.`back_balance` AS `back_balance`,`exhibition_stand`.`is_deleted` AS `is_deleted`,`exhibition_stand`.`created_on` AS `created_on`,`exhibition_stand`.`updated_on` AS `updated_on` from `exhibition_stand`
